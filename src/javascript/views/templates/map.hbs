<div class="nav" id="change-your-address">
  <div class="left-arrow"></div>
  Back
</div>
<div class="box">
  <div class="address-name">Address you entered:</div>
  <div class="address">
    {{ data.normalizedInput.line1 }}
    {{ data.normalizedInput.line2 }}
    {{ data.normalizedInput.line3 }}
    {{ data.normalizedInput.city }}
    {{ data.normalizedInput.state }}
    {{ data.normalizedInput.zip }}
  </div>
</div>
<div id="map-canvas">
</div>
<button id="more-locations">More Locations</button>
<div id="location" class="box">
  Your Closest Polling Location
  <div class="address-name">
  {{ data.pollingLocations.0.address.locationName }}
  </div>
  <div class="address-distance">
  {{ data.distance }}
  </div>
  <div class="address">
  {{ data.pollingLocations.0.address.line1 }}
  </div>
  <div class="address">
  {{ data.pollingLocations.0.address.line2 }}
  </div>
  <div class="address">
  {{ data.pollingLocations.0.address.line3 }}
  </div>
  <div class="address">
  {{ data.pollingLocations.0.address.city }}
  {{ data.pollingLocations.0.address.state }}
  {{ data.pollingLocations.0.address.zip }}
  </div>
  <a href="https://www.google.com/maps?saddr=My+Location&daddr={{data.pollingLocations}}">Directions</a>
</div>
<div id="voter-resources" class="box">
Voter Resources
<div class="right-arrow"></div>
</div>
<div class="contests">
{{#each data.contests}}
{{> contest}}
{{/each}}
</div>