<div class="contest contest-toggle">
	<div class="subsection box">
		{{ office }}<br>
		<div class="contest-type"> 
			{{ primaryParty }}
			{{ type }}
		</div>
		<span class="subsection-plus">+</span>
	</div>
	<div class="candidate-list">
		{{#if referendumSubtitle }}
		<a href="{{ referendumUrl }}">
			<div class="referendum-subtitle box">
				{{ referendumTitle }}
				<img src="./images/arrow.png" class="arrow">
				{{!-- <span class="right-arrow"><span class="right-inner-arrow right-arrow"></span></span> --}}
				{{ referendumSubtitle }}
			</div>
		</a>
		{{/if }}
		{{#each candidates}}
		{{#if candidateUrl}}
		<a href="{{ candidateUrl }}" target="_blank">
		{{/if}}
			<div class="candidate-item box">
				<div class="name">
				{{ name }}
				</div>
				{{#if candidateUrl}}
				<img src="./images/arrow.png" class="arrow">
				{{/if}}
				{{!-- <span class="right-arrow"><span class="right-inner-arrow right-arrow"></span></span> --}}
				<div class="party">
				{{ party }}
				</div>
			</div>
		{{#if candidateUrl}}
		</a>
		{{/if}}
		{{/each}}
	</div>
</div>