<div class="election-official">
  <div class="name">
  {{ name }}
  </div>
  <div class="official-title">
  {{ title }}
  </div>
  {{#if officePhoneNumber }}
  <div class="office-phone-number">
  Office Phone Number: <a href="tel:{{ officePhoneNumber }}">{{ officePhoneNumber }}</a>
  </div>
  {{/if}}
  {{#if faxNumber }}
  <div class="fax-number">
  Fax Number: <a href="tel:{{ faxNumber }}">{{ faxNumber }}</a>
  </div>
  {{/if}}
  <div class="email-address">
  <a href="mailto:{{ emailAddress }}">{{ emailAddress }}</a>
  </div>
</div>